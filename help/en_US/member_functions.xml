   <refsection><title>Description</title>
    
      <para>
         <literal>sciFLT</literal> toolbox implements the followings member functions
    :</para>

    
      <itemizedlist>
      
         <listitem>
        
            <para>Triangular</para>
      
         </listitem>

      
         <listitem>
        
            <para>Trapezoidal</para>
      
         </listitem>

      
         <listitem>
        
            <para>Gaussian</para>
      
         </listitem>

      
         <listitem>
        
            <para>Extended Gaussian</para>
      
         </listitem>

      
         <listitem>
        
            <para>Generalized Bell</para>
      
         </listitem>

      
         <listitem>
        
            <para>Sigmoidal</para>
      
         </listitem>

      
         <listitem>
        
            <para>Product of two Sigmoidal</para>
      
         </listitem>

      
         <listitem>
        
            <para>Absolute difference of tho Sigmoidal</para>
      
         </listitem>

      
         <listitem>
        
            <para>S-Shaped</para>
      
         </listitem>

      
         <listitem>
        
            <para>Z-Shaped</para>
      
         </listitem>

      
         <listitem>
        
            <para>Pi-Shaped</para>
      
         </listitem>

      
         <listitem>
        
            <para>Constant (as output for Takagi-Sugeno)</para>
      
         </listitem>

      
         <listitem>
        
            <para>Linear (as output for Takagi-Sugeno)</para>
      
         </listitem>
    
      </itemizedlist>
  
   </refsection>

  
